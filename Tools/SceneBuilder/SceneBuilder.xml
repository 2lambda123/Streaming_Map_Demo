<?xml version="1.0" encoding="utf-8"?>
<SceneBuilder>

  <module>Utility</module>
  <module>Image</module>
  <module>DB</module>
  <module>Vricon</module>
    
  
  <Version>2.16.0</Version>
  
  <InfoString>SceneBuilder configuration tool info</InfoString>
  
  <DDSTool>%s\nvcompress -bc1 %s</DDSTool>

  <TarTool>tar xvf %s -C %s</TarTool>
  
  <CopyTool>cmd /C copy %s %s</CopyTool>

  <BackFileSep>true</BackFileSep>
  
  <DatabaseAuthor>AMO</DatabaseAuthor>
  <DatabaseComment>Only for Demo Purposes and Development</DatabaseComment>

  <distributor>
    <server>
    
      <communication>
        <url>udp::65432?blocking=no&amp;nic=10.23.24.50</url>
        <!--<url>udp::65432?blocking=no&amp;nic=10.23.24.43</url>-->
      </communication>

      <connection>
        <url>tcp:%s:%d?blocking=no</url>
      </connection>

    </server>
  </distributor>
  
</SceneBuilder>
